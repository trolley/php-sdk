{
    "name": "paymentrails/php-sdk",
    "type": "library",
    "description": "Payment Rails PHP Client Library",
    "homepage": "https://github.com/PaymentRails/php-sdk",
    "keywords": [
        "PaymentRails",
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "PaymentRails",
            "homepage": "https://www.paymentrails.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.*",
        "ramsey/uuid": "*"
    },
    "autoload": {
        "psr-0": {
            "PaymentRails": "lib/"
        },
        "psr-4": {
            "PaymentRails\\": "lib/PaymentRails"
        }
    }
}
